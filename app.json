{
  "expo": {
    "name": "twiling-location",
    "slug": "twiling-location",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cargps.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "backgroundColor": {
      "light": "#d21616",
      "dark": "#3f3c3c"
    },
    "splash": {
      "image": "./assets/cargps.png",
      "resizeMode": "contain",
      "backgroundColor": {
        "light": "#d21616",
        "dark": "#3f3c3c"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cargps.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-asset", "expo-router", "expo-image-picker"],
    "assetBundlePatterns": ["**/*"],
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "scheme": "twiling-location-app-scheme"
  }
}
